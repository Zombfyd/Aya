# Environment
NODE_ENV=development
VITE_APP_ENVIRONMENT=development

# Testnet Network Configuration
VITE_APP_TESTNET_PACKAGE_ID=0x...  # Your testnet package ID
VITE_APP_TESTNET_PAYMENT_CONFIG_ID=0x...  # Your testnet payment config ID

# Mainnet Network Configuration
VITE_APP_MAINNET_PACKAGE_ID=0x...  # Your mainnet package ID
VITE_APP_MAINNET_PAYMENT_CONFIG_ID=0x...  # Your mainnet payment config ID

# Testnet Payment Recipients
VITE_APP_TESTNET_PRIMARY_RECIPIENT=0x...  # Primary recipient address for testnet
VITE_APP_TESTNET_SECONDARY_RECIPIENT=0x...  # Secondary recipient address for testnet
VITE_APP_TESTNET_TERTIARY_RECIPIENT=0x...  # Tertiary recipient address for testnet

# Mainnet Payment Recipients
VITE_APP_MAINNET_PRIMARY_RECIPIENT=0x...  # Primary recipient address for mainnet
VITE_APP_MAINNET_SECONDARY_RECIPIENT=0x...  # Secondary recipient address for mainnet
VITE_APP_MAINNET_TERTIARY_RECIPIENT=0x...  # Tertiary recipient address for mainnet

# Payment Shares Configuration (in basis points)
VITE_APP_PRIMARY_SHARE=6000    # 60%
VITE_APP_SECONDARY_SHARE=2500  # 25%
VITE_APP_TERTIARY_SHARE=1500   # 15%

# Payment Amount (in MIST)
VITE_APP_TOTAL_AMOUNT=200000000  # 0.2 SUI

# Debug Settings
VITE_APP_DEBUG_MODE=true  # Set to 'true' for development, 'false' for production
VITE_APP_LOG_LEVEL=debug  # Options: debug, info, warn, error
